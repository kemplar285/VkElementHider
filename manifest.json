{
    "name": "VK Element Hider",
    "description": "Hides chosen elements from vk.com",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://vk.com/*"
            ],
            "js": ["element_hider.js"]
        }
    ]
}